def check_token_validity():
    try:
        response = requests.get(f"https://api.telegram.org/bot{TOKEN}/getMe")
        if response.status_code == 200:
            print(f"{green}[âœ“] ØªÙˆÙƒÙ† Ø§Ù„Ø¨ÙˆØª ØµØ­ÙŠØ­")
            return True
        else:
            print(f"{red}[âœ—] ØªÙˆÙƒÙ† Ø§Ù„Ø¨ÙˆØª ØºÙŠØ± ØµØ­ÙŠØ­")
            return False
    except Exception:
        print(f"{red}[âœ—] Ø­Ø¯Ø« Ø®Ø·Ø£ Ø£Ø«Ù†Ø§Ø¡ Ø§Ù„ØªØ­Ù‚Ù‚ Ù…Ù† ØªÙˆÙƒÙ† Ø§Ù„Ø¨ÙˆØª")
        return False

# Ø§Ù„ØªØ­Ù‚Ù‚ Ù…Ù† ØµØ­Ø© Ø§Ù„ØªÙˆÙƒÙ†
